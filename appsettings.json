{
  "ssh":
  {
    "command": "mobaxterm",
    "arguments": "-newtab \"sshpass -p '{password}' ssh {username}@{hostname}\""
  },
  "sftp":
  {
    "command": "mobaxterm",
    "arguments": "-newtab \"sshpass -p '{password}' sftp {username}@{hostname}\""
  },
  "rdp":
  {
    "command": "C:\\Program Files\\Royal TS V7\\rtscli.exe",
    "arguments": "action connect-adhoc --uri {hostname} --scheme rdp --username={username} --password={password}"
  },
  "rdp2":
  {
    "command": "mobaxterm",
    "arguments": "-newtab \"wfreerdp /v:{hostname} /u:{username} /p:'{password}'\""
  },
  "winbox":
  {
    "command": "winbox64.exe",
    "arguments": "{hostname} {username} {password}"
  }
}